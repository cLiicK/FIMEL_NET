<div class="modal fade" id="modalNuevaCita" tabindex="-1" aria-labelledby="modalNuevaCitaLabel" aria-hidden="true">
    <div class="modal-dialog">
        <div class="modal-content modal-sm">
            <div class="modal-header">
                <center><strong><h5 class="modal-title">Nueva Cita</h5></strong></center>
                <button type="button" class="btn-close" data-bs-dismiss="modal" aria-label="Cerrar"></button>
            </div>
            <div class="modal-body">
                <p><strong>Fecha</strong> <input class="form-control" type="date" id="fechaCitaNueva" /></p>
                <p><strong>Hora de Inicio</strong> <input class="form-control" type="time" id="horaInicioCitaNueva" /></p>
                <p><strong>Hora Término</strong> <input class="form-control" type="time" id="horaTerminoCitaNueva" /></p>
                <p><strong>Nombre y apellido</strong> <input class="form-control" id="nombreCitaNueva" /></p>
                <p><strong>Correo</strong> <input class="form-control" id="correoCitaNueva" /></p>
                <p><strong>Telefono</strong> <input class="form-control" id="telefonoCitaNueva" /></p>
                <p><strong>Nota</strong> <input class="form-control" id="notaCitaNueva" /></p>
            </div>
            <div class="modal-footer">
                <button class="btn btn-blue" id="btnGuardarCita" onclick="ModuloHorarioCita.GuardarCita()">Guardar</button>
            </div>
        </div>
    </div>
</div>